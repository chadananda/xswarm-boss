{
  "name": "xswarm-boss",
  "version": "0.1.0",
  "private": true,
  "description": "AI Orchestration Layer for Multi-Project Development",
  "type": "module",
  "scripts": {
    "postinstall": "node scripts/install-stripe-cli.js",
    "build": "pnpm -r run build",
    "test": "node tests/test-runner.js",
    "test:unit": "node tests/test-runner.js --filter=unit",
    "test:integration": "node tests/test-runner.js --filter=integration",
    "test:e2e:full": "node tests/test-runner.js --filter=e2e",
    "test:performance": "node tests/test-runner.js --filter=performance",
    "test:security": "node tests/test-runner.js --filter=security",
    "test:verbose": "node tests/test-runner.js --verbose",
    "test:ci": "node tests/test-runner.js --output=test-results.json",
    "dev": "docker compose up -d",
    "clean": "pnpm -r run clean && cargo clean",
    "lint": "pnpm -r run lint",
    "format": "prettier --write .",
    "setup:env": "node scripts/setup-env.js",
    "setup:db": "node scripts/migrate-db.js",
    "deploy": "./deploy.sh",
    "deploy:dev": "./deploy.sh dev",
    "deploy:staging": "./deploy.sh staging",
    "deploy:production": "./deploy.sh production",
    "animate:sauron": "node scripts/animate-sauron.js",
    "animate:sauron:multi": "node scripts/animate-sauron-multi-try.js",
    "animate:sauron:luma": "node scripts/animate-sauron-luma.js",
    "animate:sauron:leonardo": "node scripts/animate-sauron-leonardo.js",
    "animate:sauron:runway": "node scripts/animate-sauron-runway.js",
    "test:twilio": "node scripts/test-twilio.js",
    "test:sendgrid": "node scripts/test-sendgrid.js",
    "test:stripe": "node scripts/test-stripe.js",
    "test:webhooks": "node scripts/test-webhooks.js",
    "test:webhooks:all": "node scripts/test-webhooks.js --all",
    "test:moshi": "node scripts/test-moshi-websocket.js",
    "test:e2e": "node scripts/test-end-to-end.js",
    "test:e2e:email": "node scripts/test-end-to-end.js email",
    "test:e2e:sms": "node scripts/test-end-to-end.js sms",
    "test:e2e:voice": "node scripts/test-end-to-end.js voice",
    "test:e2e:multi": "node scripts/test-end-to-end.js multi",
    "test:e2e:database": "node scripts/test-end-to-end.js database",
    "test:e2e:dashboard": "node scripts/test-end-to-end.js dashboard",
    "test:e2e:error": "node scripts/test-end-to-end.js error",
    "test:email-self": "node scripts/test-boss-email-self.js",
    "test:sms-conversation": "node scripts/test-sms-conversation.js",
    "test:voice-conversation": "node scripts/test-voice-conversation.js",
    "test:database-persistence": "node scripts/test-database-persistence.js",
    "test:dashboard-live": "node scripts/test-dashboard-live.js",
    "dev:server": "pnpm --filter @xswarm/server run dev",
    "dev:webhooks": "node scripts/dev-webhook-forward.js",
    "dev:webhook-server": "node scripts/test-webhook-server.js",
    "deploy:server": "pnpm --filter @xswarm/server run deploy",
    "setup:secrets": "node scripts/sync-secrets.js",
    "setup:stripe": "node scripts/setup-stripe-products.js",
    "setup:webhooks": "node scripts/setup-stripe-webhooks.js",
    "setup:twilio": "node scripts/setup-twilio-webhook.js",
    "setup:sendgrid": "node scripts/setup-sendgrid.js",
    "deploy:admin": "source .env && cd admin-pages && npx wrangler pages deploy . --project-name xswarm-signup --commit-dirty=true",
    "deploy:pages": "npm run deploy:admin",
    "deploy:check": "node deployment/scripts/pre-deploy-check.js",
    "deploy:migrate": "node deployment/scripts/migrate-all.js",
    "deploy:prod": "node deployment/scripts/deploy-production.js --env=production",
    "deploy:verify": "node deployment/scripts/verify-deployment.js",
    "deploy:rollback": "node deployment/scripts/rollback.js",
    "deploy:list": "node deployment/scripts/rollback.js --list"
  },
  "dependencies": {
    "@libsql/client": "^0.5.6",
    "@sendgrid/mail": "^8.1.6",
    "dotenv": "^16.3.1",
    "lumaai": "^1.18.0",
    "replicate": "^0.25.2",
    "smol-toml": "^1.3.0",
    "stripe": "^17.5.0",
    "twilio": "^5.10.3"
  },
  "devDependencies": {
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "wrangler": "^4.45.2",
    "ws": "^8.18.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "keywords": [
    "ai",
    "orchestration",
    "voice-assistant",
    "rust",
    "developer-tools"
  ],
  "author": "xSwarm Contributors <hello@xswarm.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xswarm-dev/xswarm-boss.git"
  },
  "bugs": {
    "url": "https://github.com/xswarm-dev/xswarm-boss/issues",
    "email": "hello@xswarm.dev"
  },
  "homepage": "https://xswarm.dev"
}
